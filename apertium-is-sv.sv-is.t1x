<?xml version="1.0" encoding="utf-8"?>

<transfer default="chunk">

  <section-def-cats>
    <def-cat n="nom">
      <cat-item tags="n.*"/>
      <cat-item tags="np.*"/>
    </def-cat>
    <def-cat n="nom_ind"> 					<!--noun, wiðout article-->
      <cat-item tags="n.*.ind"/>
      <cat-item tags="n.*.ind.*"/>
    </def-cat>
    <def-cat n="nom_def"> 					<!--noun, wið article-->
      <cat-item tags="n.*.def"/>
      <cat-item tags="n.*.def.*"/>
    </def-cat>
    <def-cat n="num">
      <cat-item tags="num.*"/>
    </def-cat>
    <def-cat n="inf"> 					<!--infinitive-->
      <cat-item tags="vblex.*.inf.*"/>
      <cat-item tags="vblex.*.inf"/>
      <cat-item tags="vbser.*.inf.*"/>
      <cat-item tags="vbser.*.inf"/>
      <cat-item tags="vbhaver.*.inf.*"/>
      <cat-item tags="vbhaver.*.inf"/>
      <cat-item tags="vbmod.*.inf.*"/>
      <cat-item tags="vbmod.*.inf"/>
      <cat-item tags="vaux.*.inf.*"/>
      <cat-item tags="vaux.*.inf"/>
    </def-cat>
    <def-cat n="vfin"> 					<!--finite verb-->
      <cat-item tags="vblex.*.pres.*"/>
      <cat-item tags="vblex.*.past.*"/>
      <cat-item tags="vbser.pres.*"/>
      <cat-item tags="vbser.past.*"/>
      <cat-item tags="vbhaver.pres.*"/>
      <cat-item tags="vbhaver.past.*"/>
      <cat-item tags="vbmod.pres.*"/>
      <cat-item tags="vbmod.past.*"/>
      <cat-item tags="vaux.pres.*"/>
      <cat-item tags="vaux.past.*"/>
    </def-cat>

   <def-cat n="prep">
      	<cat-item tags="pr"/>
    </def-cat>

    <def-cat n="prgen">    <!--prepositions that govern the genitive case-->
      	<cat-item lemma="till" tags="pr"/>
		<cat-item lemma="inom" tags="pr"/> 
		<cat-item lemma="bland" tags="pr"/> 
		<cat-item lemma="mellan" tags="pr"/> 
		<cat-item lemma="utan" tags="pr"/> 
		<cat-item lemma="på grund av" tags="pr"/> 
    </def-cat>

    <def-cat n="adj_ind"> 					<!--adjective indef, strong declension-->
      <cat-item tags="adj.*.ind"/>
    </def-cat>
    <def-cat n="adj_def"> 					<!--adjective def, weak declension-->
      <cat-item tags="adj.*.def"/>
    </def-cat>
    <def-cat n="adj_ord"> 					<!--adjective-->
      <cat-item tags="adj.ord.*"/>
    </def-cat>
    <def-cat n="adj"> 					<!--adjective-->
      <cat-item tags="adj.*"/>
    </def-cat>
  <def-cat n="mer">
     <cat-item lemma="mer" tags="*"/>
  </def-cat>
  <def-cat n="mest">
     <cat-item lemma="mest" tags="*"/>
  </def-cat>


