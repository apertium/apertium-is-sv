	<rule comment="REGLA: ADJORD  | först → fyrstur">   <!-- ORDINAL NUMERALS --> 
		     <pattern>
		       <pattern-item n="adj_ord"/>
		     </pattern>
		     <action>
		       <call-macro n="firstWord">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="conv_gender_temp">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="conv_number2">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="set_defnes_ind">
		         <with-param pos="1"/>
		       </call-macro>
		       <out>
		         <chunk name="ADJECTIVE" case="caseFirstWord">    
		           <tags>
		             <tag><lit-tag v="SA"/></tag>              
		              <tag><clip pos="1" side="tl" part="gen"/></tag>
		              <tag><clip pos="1" side="tl" part="nbr"/></tag>
		              <tag><lit-tag v="nom"/></tag>
		              <tag><clip pos="1" side="tl" part="defnes"/></tag>
		            </tags>
		            <lu>
		              <clip pos="1" side="tl" part="lemh"/>
		              <lit-tag v="adj"/>  
		              <lit-tag v="ord"/>  
		              <clip pos="1" side="tl" part="gen" link-to="2"/>
		              <clip pos="1" side="tl" part="nbr" link-to="3"/>
		              <lit-tag v="nom"/>
		              <lit-tag v="ind"/>
<!--		              <clip pos="1" side="tl" part="defnes" link-to="6"/>  PROBA!-->
		              <clip pos="1" side="tl" part="lemq"/>
		            </lu>
		          </chunk>
		         </out>
		 </action>
	</rule>

	<rule comment="REGLA: ADJ  | vacker → fallegur">   <!-- --> 
		     <pattern>
		       <pattern-item n="adj"/>
		     </pattern>
		     <action>
		       <call-macro n="firstWord">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="conv_gender_temp">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="conv_number2">
		         <with-param pos="1"/>
		       </call-macro>
		       <call-macro n="set_defnes_ind">
		         <with-param pos="1"/>
		       </call-macro>
		    <choose>
	         <when>
	           <test>
				<and>
	             <equal>
	               <clip pos="1" side="tl" part="a_adj"/>
	               <lit-tag v="adj.unsint"/>
	             </equal>
	             <equal>
	               <clip pos="1" side="sl" part="grau"/>
	               <lit-tag v="comp"/>
	             </equal>
				</and>
	           </test>
		       <out>
		         <chunk name="ADJ_COMP_UNSINT" case="caseFirstWord">    <!--olämpligare - meira óviðeigandi --> 
		           <tags>
		             <tag><lit-tag v="SA"/></tag>     
		              <tag><clip pos="1" side="tl" part="grau"/></tag>
		              <tag><clip pos="1" side="tl" part="gen"/></tag>
		              <tag><clip pos="1" side="tl" part="nbr"/></tag>
		              <tag><lit-tag v="nom"/></tag>
		              <tag><lit-tag v="ind"/></tag>
		            </tags>
		            <lu>
		              <lit v="meira"/>
		              <lit-tag v="adv"/>
		            </lu>
		            <b pos="0"/>
		            <lu>
		              <clip pos="1" side="tl" part="lemh"/>
		              <lit-tag v="adj"/>
		              <lit-tag v="pst"/>
		              <clip pos="1" side="tl" part="gen" link-to="3"/>
		              <clip pos="1" side="tl" part="nbr" link-to="4"/>
		              <lit-tag v="nom"/>
		              <lit-tag v="ind"/>
		              <clip pos="1" side="tl" part="lemq"/>
		            </lu>
		          </chunk>
		         </out>
	         </when>
	         <when>
	           <test>
				<and>
	             <equal>
	               <clip pos="1" side="tl" part="a_adj"/>
	               <lit-tag v="adj.unsint"/>
	             </equal>
	             <equal>
	               <clip pos="1" side="sl" part="grau"/>
	               <lit-tag v="sup"/>
	             </equal>
				</and>
	           </test>
		       <out>
		         <chunk name="ADJ_SUP_UNSINT" case="caseFirstWord">   <!--olämpligast - mest óviðeigandi --> 
		           <tags>
		             <tag><lit-tag v="SA"/></tag>     
		              <tag><clip pos="1" side="tl" part="grau"/></tag>
		              <tag><clip pos="1" side="tl" part="gen"/></tag>
		              <tag><clip pos="1" side="tl" part="nbr"/></tag>
		              <tag><lit-tag v="nom"/></tag>
		              <tag><clip pos="1" side="tl" part="defnes"/></tag>
		            </tags>
		            <lu>
		              <lit v="mest"/>
		              <lit-tag v="adv"/>
		            </lu>
		            <b pos="0"/>
		            <lu>
		              <clip pos="1" side="tl" part="lemh"/>
		              <lit-tag v="adj"/>
		              <lit-tag v="pst"/>
		              <clip pos="1" side="tl" part="gen" link-to="3"/>
		              <clip pos="1" side="tl" part="nbr" link-to="4"/>
		              <lit-tag v="nom"/>
		              <clip pos="1" side="tl" part="nbr" link-to="6"/>   <!--
NB!!!								ha?????? NB!!!!!
		              -->
		              <clip pos="1" side="tl" part="lemq"/>
		            </lu>
		          </chunk>
		         </out>
	         </when>
			<otherwise>
		       <out>
		         <chunk name="ADJECTIVE" case="caseFirstWord">    
		           <tags>
		             <tag><lit-tag v="SA"/></tag>              
		              <tag><clip pos="1" side="tl" part="grau"/></tag>
		              <tag><clip pos="1" side="tl" part="gen"/></tag>
		              <tag><clip pos="1" side="tl" part="nbr"/></tag>
		              <tag><lit-tag v="nom"/></tag>
		              <tag><clip pos="1" side="tl" part="defnes"/></tag>
		            </tags>
		            <lu>
		              <clip pos="1" side="tl" part="lemh"/>
		              <lit-tag v="adj"/>  <!--to avoid sint/unsint            -->
		              <clip pos="1" side="tl" part="grau" link-to="2"/>
		              <clip pos="1" side="tl" part="gen" link-to="3"/>
		              <clip pos="1" side="tl" part="nbr" link-to="4"/>
		              <lit-tag v="nom"/>
		              <lit-tag v="ind"/>   		<!-- is this correct??? -->
<!--		              <clip pos="1" side="tl" part="defnes" link-to="6"/>  PROBA!-->
		              <clip pos="1" side="tl" part="lemq"/>
		            </lu>
		          </chunk>
		         </out>
			</otherwise>
			</choose>
		 </action>
	</rule>



    <rule comment="regla: num">
      <pattern>
        <pattern-item n="num"/>
      </pattern>
      <action>
        <out>
          <chunk name="num" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
              <tag><clip pos="1" side="tl" part="gen"/></tag>
              <tag><clip pos="1" side="tl" part="nbr"/></tag>
              <tag><lit-tag v="nom"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_num"/>
              <clip pos="1" side="tl" part="gen" link-to="2"/>
              <clip pos="1" side="tl" part="nbr" link-to="3"/>
              <lit-tag v="4"/>
              <clip pos="1" side="tl" part="lemq"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>



    <rule comment="regla: nom">
      <pattern>
        <pattern-item n="nom"/>
      </pattern>
      <action>
