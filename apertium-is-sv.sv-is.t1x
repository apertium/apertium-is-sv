  <section-def-macros>
	
    <def-macro n="conv_case_to_acc" npar="1" > 	<!-- MACRO: convert nom to acc -->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="acc"/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="conv_case_to_dat" npar="1" > 		<!-- MACRO: convert nom to dat-->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="acc"/>
          </let>
        </when>
      </choose>
    </def-macro>

    <def-macro n="conv_case_to_gen" npar="1" > 		<!-- MACRO: convert nom to gen-->	
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="cas"/><lit-tag v="nom"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="cas"/>
            <lit-tag v="gen"/>
          </let>
        </when>
      </choose>
    </def-macro>


    <def-macro n="conv_gender_temp" npar="1" > 	<!-- MACRO: used for adjectives (pst.pl, comp, sup: mfn) !!! -->	
      <choose>
        <when>
          <test>
				  <or>
	                <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="ut"/></equal>
	                <equal><clip pos="1" side="sl" part="gen"/><lit-tag v="mfn"/></equal>
				  </or>
          </test>
          <let>
            <clip pos="1" side="tl" part="gen"/>
            <lit-tag v="m"/>
          </let>
        </when>
	 </choose>
    </def-macro>


    <def-macro n="conv_number2" npar="1" > 	<!-- MACRO: used for adjectives !!! -->	
      <choose>
        <when>
          <test>
	              <equal><clip pos="1" side="sl" part="nbr"/><lit-tag v="sp"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="nbr"/>
            <lit-tag v="sg"/>
          </let>
        </when>
	 </choose>
    </def-macro>

    <def-macro n="conv_number1" npar="1" > 	<!-- MACRO: rewrite!!! -->	
      <choose>
        <when>
          <test>
			<or>
			      <and>
	                  <equal><clip pos="1" side="sl" part="defnes"/><lit-tag v="def"/></equal>
      	              <equal><clip pos="1" side="sl" part="grau"/><lit-tag v="pst"/></equal>
               	  </and>
                  <equal><clip pos="1" side="sl" part="grau"/><lit-tag v="comp"/></equal>
                  <equal><clip pos="1" side="sl" part="grau"/><lit-tag v="sup"/></equal>
    	    </or>
          </test>
          <let>
            <clip pos="1" side="tl" part="nbr"/>
            <lit-tag v="sg"/>
          </let>
        </when>
	 </choose>
    </def-macro>

    <def-macro n="set_defnes_ind" npar="1" > 	<!-- MACRO: for adj.comp with no tag for defnes, does it work? -->	
      <choose>
        <when>
          <test>
      	       <equal><clip pos="1" side="sl" part="grau"/><lit-tag v="comp"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="defnes"/>
            <lit-tag v="ind"/>
          </let>
        </when>
	 </choose>
    </def-macro>


